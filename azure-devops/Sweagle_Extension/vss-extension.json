{
    "manifestVersion": 1,
    "id": "sweagle-release-task",
    "name": "sweagle",
    "version": "1.0.1",
    "publisher": "sweagle",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "ServiceNow Configuration Data Management module for Azure DevOps",
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
      "details": {
        "path": "overview.md"
      }
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "tags": [
        "CICD", "configuration", "configData", "compliance",
        "devops",
        "iac", "infrastructure as code",
        "key value",
        "quality",
        "secret", "security", "sre",
        "utility task",
        "validation", "vault"
    ],
    "contributions": [
        {
            "id": "custom-sweagle-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}
